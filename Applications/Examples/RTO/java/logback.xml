<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{50} - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="WARN">
		<appender-ref ref="STDOUT" />
	</root>
	<logger name="org.jose4j" level="INFO"/>
</configuration>
